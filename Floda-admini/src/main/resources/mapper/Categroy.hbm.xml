<?xml version = "1.0" encoding = "utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="pojo.Categroy" table="tb_categroy">
        <id name="cate_id" column="cate_id">
            <generator class="native"></generator>
        </id>
        <property name="cate_desc" column="cate_desc"/>
        <property name="cate_name" column="cate_name"/>
    </class>
</hibernate-mapping>